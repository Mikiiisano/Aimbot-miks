local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Crear una función para dar acceso de admin
function giveAdmin()
    -- Asegúrate de que el LocalPlayer sea el administrador
    if LocalPlayer.Name == "tu_usuario" then -- Reemplaza 'tu_usuario' con tu nombre de usuario
        LocalPlayer:Kick("Has sido hecho admin.")
    else
        print("No tienes permiso para ser admin.")
    end
end

-- Ejecutar la función
giveAdmin()
